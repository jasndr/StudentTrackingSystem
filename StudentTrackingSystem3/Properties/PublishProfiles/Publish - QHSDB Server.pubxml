<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>qhsdb.jabsomit.hawaii.edu/StudentTrackingSystem</SiteUrlToLaunchAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>qhsdb.jabsomit.hawaii.edu</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site\StudentTrackingSystem</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ApplicationDbContext" Order="2" Enabled="True">
          <Destination Path="Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\ApplicationDbContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="SchoolContext" Order="3" Enabled="False">
          <Destination Path="Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="StudentTrackingSystem3.DAL.SchoolContext, StudentTrackingSystem3" MigrationConfiguration="StudentTrackingSystem3.Migrations.Configuration, StudentTrackingSystem3" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="ApplicationDbContext-Web.config Connection String">
      <ParameterValue>Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="SchoolContext-Web.config Connection String">
      <ParameterValue>Data Source=qhsdb;Initial Catalog=MSCTR;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>