          <Connection AppSettingsObjectName="Web.config" AppSettingsPropertyName="SchoolContext_DatabasePublish" ConnectionStringObject="" IsAppSettingsProperty="true" Modifier="Assembly" Name="SchoolContext_DatabasePublish (Web.config)" ParameterPrefix="@" PropertyReference="AppConfig.System.Configuration.ConfigurationManager.0.ConnectionStrings.SchoolContext_DatabasePublish.ConnectionString" Provider="System.Data.SqlClient" />
