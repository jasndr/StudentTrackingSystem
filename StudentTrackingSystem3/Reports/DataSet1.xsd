            <MainSource>
              <DbSource ConnectionRef="SchoolContext (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                         s.Id, s.StudentNumber AS StudentID, s.FirstName, s.MiddleName, s.LastName, s.SchoolEmail, s.OtherEmail, s.Phone, gender.Name AS Gender, 
                         REPLACE(REPLACE(REPLACE('&lt;' + STUFF
                             ((SELECT        CASE WHEN pv2.DegreeTypesID IS NULL 
                                                          THEN 'N/A' WHEN pv2.DegreeTypesID = '' THEN 'N/A' ELSE '; -' + degTypes.Name + ' in ' + pv2.Title + ' (' + pv2.SchoolName + ', ' + CAST(YEAR(pv2.DateOfAward)
                                                           AS nvarchar(4)) + ')' END AS Expr1
                                 FROM            G_PrevDegree AS pv2 INNER JOIN
                                                          G_Student AS s2 ON s.Id = s2.Id AND s2.Id = pv2.StudentID LEFT OUTER JOIN
                                                          G_CommonFields AS degTypes ON degTypes.ID = pv2.DegreeTypesID FOR XML PATH('')), 1, 1, ''), '&lt;Expr1&gt;', ''), '&lt;/Expr1&gt;', ''), ';', '') 
                         AS DegreeAtAdmission, REPLACE(REPLACE(REPLACE('&lt;' + STUFF
                             ((SELECT        CASE WHEN r2.Name IS NULL THEN 'N/A' WHEN r2.Name = '' THEN 'N/A' ELSE '; -' + r2.Name END AS Expr1
                                 FROM            G_Races AS r2 INNER JOIN
                                                          G_PersonRaces AS pr2 ON pr2.RaceID = r2.Id INNER JOIN
                                                          G_Student AS s2 ON s.Id = s2.Id AND s2.Id = pr2.StudentID FOR XML PATH('')), 1, 1, ''), '&lt;Expr1&gt;', ''), '&lt;/Expr1&gt;', ''), ';', '') AS RaceEthnicity, 
                         s.RaceOther, degreeProgram.Name AS DegreeProgram, track.Name AS Track, plans.Name AS [Plan], degreeStartSem.Name AS DegreeStartSemester, 
                         s.DegreeStartYear, gradSem.Name AS DegreeAwardedSemester, grad.DegreeEndYear AS DegreeAwardedYear
FROM            G_Student AS s LEFT OUTER JOIN
                         G_PrevDegree AS pd ON pd.StudentID = s.Id INNER JOIN
                         G_CommonFields AS gender ON gender.ID = s.GendersId INNER JOIN
                         G_CommonFields AS degreeProgram ON degreeProgram.ID = s.DegreeProgramsId INNER JOIN
                         G_CommonFields AS track ON track.ID = s.TracksId INNER JOIN
                         G_CommonFields AS plans ON plans.ID = s.PlansId INNER JOIN
                         G_CommonFields AS degreeStartSem ON degreeStartSem.ID = s.DegreeStartSemsId LEFT OUTER JOIN
                         G_Graduation AS grad ON grad.StudentID = s.Id LEFT OUTER JOIN
                         G_CommonFields AS gradSem ON gradSem.ID = grad.DegreeEndSemsId
WHERE        ((DATEFROMPARTS(s.DegreeStartYear, 01, 01) &lt; @FromDate AND NOT (DATEFROMPARTS(grad.DegreeEndYear, 1, 1) &lt; @FromDate) OR
                         DATEFROMPARTS(s.DegreeStartYear, 01, 01) &lt; @FromDate AND grad.DegreeEndYear IS NULL OR
                         DATEFROMPARTS(s.DegreeStartYear, 01, 01) &gt;= @FromDate AND DATEFROMPARTS(s.DegreeStartYear, 01, 01) &lt;= @ToDate) AND 
                         NOT (DATEFROMPARTS(s.DegreeStartYear, 01, 01) &gt; @ToDate) OR
                         ISNULL(@FromDate, '') = '' OR
                         ISNULL(@ToDate, '') = '') AND (s.FirstName + ' ' + s.LastName = @Student OR
                         ISNULL(@Student, '') = '') AND ((CASE WHEN (gradSem.Name IS NULL AND grad.DegreeEndYear IS NULL) THEN 'Current' ELSE 'Former' END) = @CurrentFormer OR
                         ISNULL(@CurrentFormer, '') = '')
ORDER BY s.DegreeStartYear DESC</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="FromDate" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="AnsiString" Direction="Input" ParameterName="@FromDate" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="ToDate" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="AnsiString" Direction="Input" ParameterName="@ToDate" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
